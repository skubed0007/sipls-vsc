{
  "Function Declaration": {
    "prefix": "fun",
    "body": [
      "fun ${1:function_name}(${2:type1} ${3:param1}) @ ${6:return_type} {",
      "  ${7:body}",
      "  ret ${8:return_value};",
      "}"
    ],
    "description": "Function declaration with parameters and return type"
  },
  "Variable Declaration": {
    "prefix": "let",
    "body": [
      "let ${1:variable_name} @ ${2:type} = ${3:value};"
    ],
    "description": "Variable declaration with type"
  },
  "Import Statement": {
    "prefix": "bring",
    "body": [
      "bring \"${1:module_name}\";"
    ],
    "description": "Import a module"
  },
  "If Statement": {
    "prefix": "if",
    "body": [
      "if (${1:condition}) {",
      "  ${2:body}",
      "}"
    ],
    "description": "If statement"
  },
  "If-Else Statement": {
    "prefix": "ifelse",
    "body": [
      "if (${1:condition}) {",
      "  ${2:body}",
      "} else {",
      "  ${3:else_body}",
      "}"
    ],
    "description": "If-else statement"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while (${1:condition}) {",
      "  ${2:body}",
      "}"
    ],
    "description": "While loop"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for (${1:initialization}; ${2:condition}; ${3:increment}) {",
      "  ${4:body}",
      "}"
    ],
    "description": "For loop"
  },
  "C Code Block": {
    "prefix": "@",
    "body": [
      "@c {",
      "  ${1:c_code}",
      "}"
    ],
    "description": "C code block"
  },
  "Struct Declaration": {
    "prefix": "struct",
    "body": [
      "struct ${1:struct_name} {",
      "}"
    ],
    "description": "Struct declaration"
  },
  "Enum Declaration": {
    "prefix": "enum",
    "body": [
      "enum ${1:enum_name} {",
      "}"
    ],
    "description": "Enum declaration"
  },
  "Return Statement": {
    "prefix": "ret",
    "body": [
      "ret ${1:value};"
    ],
    "description": "Return statement"
  },
  "Single Line Comment": {
    "prefix": "//",
    "body": [
      "// ${1:comment}"
    ],
    "description": "Single line comment"
  },
  "Block Comment": {
    "prefix": "/*",
    "body": [
      "/* ${1:comment} */"
    ],
    "description": "Block comment"
  },
  "Main Function": {
    "prefix": "main",
    "body": [
      "fun main() @ i32 {",
      "  ${1:body}",
      "  ret 0;",
      "}"
    ],
    "description": "Main function with return type"
  },
  "Type Declaration": {
    "prefix": "type",
    "body": [
      "type ${1:type_name} = ${2:type_definition};"
    ],
    "description": "Type declaration"
  },
  "Break Statement": {
    "prefix": "break",
    "body": [
      "break;"
    ],
    "description": "Break statement"
  },
  "Continue Statement": {
    "prefix": "continue",
    "body": [
      "continue;"
    ],
    "description": "Continue statement"
  },
  "Print Statement": {
    "prefix": "print",
    "body": [
      "print(\"${1:text}\");"
    ],
    "description": "Print statement"
  }
}
